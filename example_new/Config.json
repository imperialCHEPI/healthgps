{
    "version": 2,
    "inputs": {
        "dataset": {
            "name": "France.DataFile.csv",
            "format": "csv",
            "delimiter": ",",
            "encoding": "ASCII",
            "columns": {
                "Year": "integer",
                "Gender": "integer",
                "Age": "integer",
                "Age2": "integer",
                "Age3": "integer",
                "SES": "double",
                "PA": "double",
                "Protein": "double",
                "Sodium": "double",
                "Fat": "double",
                "Energy": "double",
                "BMI": "double"
            }
        },
        "settings": {
            "country_code": "FRA",
            "size_fraction": 0.0001,
            "age_range": [0, 100]
        }
    },
    "modelling": {
        "ses_model": {
            "function_name": "normal",
            "function_parameters": [0.0, 1.0]
        },
        "risk_factors": [
            {
                "name": "Gender",
                "level": 0,
                "range": [0, 1]
            },
            {
                "name": "Age",
                "level": 0,
                "range": [1, 87]
            },
            {
                "name": "SES",
                "level": 0,
                "range": [-2.316299, 2.296689]
            },
            {
                "name": "PhysicalActivity",
                "level": 2,
                "range": [22.22314, 9765.512]
            },
            {
                "name": "EnergyIntake",
                "level": 2,
                "range": [1326.14051277588, 7522.496]
            },
            {
                "name": "BMI",
                "level": 3,
                "range": [13.88, 39.48983]
            }
        ],
        "risk_factor_models": {
            "static": "StaticLinear.json",
            "dynamic": "KevinHall.json"
        },
        "baseline_adjustments": {
            "format": "csv",
            "delimiter": ",",
            "encoding": "UTF8",
            "file_names": {
                "factorsmean_male": "FactorsMean.Male.csv",
                "factorsmean_female": "FactorsMean.Female.csv"
            }
        }
    },
    "running": {
        "seed": [123454321],
        "start_time": 2010,
        "stop_time": 2015,
        "trial_runs": 1,
        "sync_timeout_ms": 15000,
        "diseases": [
            "alzheimer",
            "asthma",
            "colorectalcancer",
            "diabetes",
            "lowbackpain",
            "osteoarthritisknee"
        ],
        "interventions": {
            "active_type_id": "simple",
            "types": {
                "simple": {
                    "active_period": {
                        "start_time": 2022,
                        "finish_time": null
                    },
                    "impact_type": "absolute",
                    "impacts": []
                }
            }
        }
    },
    "output": {
        "comorbidities": 5,
        "folder": "${HOME}/healthgps/results/france",
        "file_name": "HealthGPS_Result_{TIMESTAMP}.json"
    }
}
