{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "https://raw.githubusercontent.com/imperialCHEPI/healthgps/main/schemas/v1/data_index/country.json",
    "type": "object",
    "properties": {
        "format": {
            "const": "csv"
        },
        "delimiter": {
            "const": ","
        },
        "encoding": {
            "const": "UTF8"
        },
        "description": {
            "type": "string"
        },
        "source": {
            "type": "string"
        },
        "url": {
            "type": "string",
            "format": "uri"
        },
        "license": {
            "type": "string"
        },
        "path": {
            "type": "string"
        },
        "file_name": {
            "type": "string"
        }
    },
    "required": [
        "path",
        "file_name"
    ],
    "additionalProperties": false
}
