                "name": "Breast Cancer"
            },
            {
                "group": "cancer",
                "id": "colorectalcancer",
                "name": "Colorectum Cancer"
            },
            {
                "group": "cancer",
                "id": "esophaguscancer",
                "name": "Esophagus Cancer"
            },
            {
                "group": "cancer",
