    },
    "inputs": {
        "dataset": {
            "name": "Finch.DataFile.csv",
            "format": "csv",
            "delimiter": ",",
            "encoding": "ASCII",
            "columns": {
                "Gender": "integer",
                "Age": "integer",
                "Age2": "integer",
                "Age3": "integer",
                "Region": "integer",
